  //Gravity 0.09
  yScale 0.8
  xscale 1.25
  alpha 0.8
  +CLIENTSIDEONLY
  States
  {
  Spawn:
    BL0D A 0 
	BLOD A 0 A_PlaySound("flesh/hit")
    BL0D A 0 A_Jump(128,"Spawn2","Spawn3","Spawn4","Spawn5","Spawn6")
    //BL0D A 0 A_Jump(256,"Spawn1")
	//BL0D A 35
	//Stop
	Goto Spawn1
  Spawn1:
	BL0D A 0 //A_PlaySound("flesh/hit")
    BL0D A 1
    BL0D ABBBCCCCC 1 A_Fadeout(0.09)
	Stop
	
  Spawn2:
	BL0D A 0 //A_PlaySound("flesh/hit")
    BL0D ABB 1
    BL0D CCCC 1 A_Fadeout(0.2)
    Stop
		
  Spawn3:
	BL0D A 0 //A_PlaySound("flesh/hit")
    BL0D AABBCCCC 1 A_Fadeout(0.1)
    Stop
	
  Spawn4:
	BL0D A 0 //A_PlaySound("flesh/hit")
    BL0D A 1
    BL0D DEEEFFFFF 1 A_Fadeout(0.09)
	Stop
	
  Spawn5:
	BL0D A 0 //A_PlaySound("flesh/hit")
    BL0D DEE 1
    BL0D FFFF 1 A_Fadeout(0.2)
    Stop
		
  Spawn6:
	BL0D A 0 //A_PlaySound("flesh/hit")
    BL0D DDEEFFFF 1 A_Fadeout(0.1)
    Stop
  }
}

