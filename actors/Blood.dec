ACTOR AeniBloodEffectSpawner
{
  +NOTELEPORT
  Radius 1
  Height 1
  +NOBLOCKMAP
  +NOTELEPORT
  //+NOGRAVITY
  Gravity 0.8
  +NOCLIP
  -SOLID
  -SHOOTABLE
  +DONTSPLASH
  RENDERSTYLE normal //none
  scale 0.8
  States
  {
  Spawn:
    TNT1 A 0
	TNT1 A 0 //A_FaceTarget
	BLOD A 0 A_Jump(127, "Spawn2")
	BLOD A 0 A_Jump(256, "Spawn1")
	//TNT1 A 35
	//Stop
  Spawn1:
    TNT1 AAAA 0 Bright A_SpawnItemEx ("BludEffect1b", 0,0,0, random(-65,65)*0.01,random(-65,65)*0.01,random(-65,65)*0.01, 0, SXF_NOCHECKPOSITION|SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION, 0)
	Goto Death//Spray
  Spawn2:
    TNT1 AAAA 0 Bright A_SpawnItemEx ("BludEffect1", 0,0,0, random(-30,30)*0.01,random(-30,30)*0.01,random(-30,30)*0.01, 0, SXF_NOCHECKPOSITION|SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION, 0)
	Goto Death//Spray
	  
  Spray:
	  BLOD A 0 A_Jump(127, "Death","Spray2")
	  BLOD A 0 //A_PlaySound("flesh/gush")
	  BLOD A 0 //A_SetAngle(random(-180,180))
      BL0D AAA 1 Bright A_SpawnItemEx("BludEffect4", 0,0,0, 0,0,0, random(-2,2), SXF_NOCHECKPOSITION|SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION)//|SXF_ABSOLUTEANGLE)
	  BLOD A 0 A_Jump(16, "Death")
	  BLOD A 0 //A_PlaySound("flesh/gush")
	  BLOD A 0 //A_SetAngle(random(-180,180))
      BL0D BBB 1 Bright A_SpawnItemEx("BludEffect3", 0,0,0, 0,0,0, random(-4,4), SXF_NOCHECKPOSITION|SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION)//|SXF_ABSOLUTEANGLE)
	  BLOD A 0 A_Jump(8, "Death")
	  BLOD A 0 //A_PlaySound("flesh/gush")
	  BLOD A 0 //A_SetAngle(random(-180,180))
      BL0D CCCC 1 Bright A_SpawnItemEx("BludEffect2", 0,0,0, 0,0,0, random(-8,8), SXF_NOCHECKPOSITION|SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION)//|SXF_ABSOLUTEANGLE)
      Stop
  Spray2:
	  BLOD A 0 //A_PlaySound("flesh/gush")
	  BLOD A 0 //A_SetAngle(random(-180,180))
      BL0D DDD 1 Bright A_SpawnItemEx("BludEffect4", 0,0,0, 0,0,0, random(-8,8), SXF_NOCHECKPOSITION|SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION)//|SXF_ABSOLUTEANGLE)
	  BLOD A 0 A_Jump(16, "Death")
	  BLOD A 0 //A_PlaySound("flesh/gush")
	  BLOD A 0 //A_SetAngle(random(-180,180))
      BL0D EEE 1 Bright A_SpawnItemEx("BludEffect3", 0,0,0, 0,0,0, random(-16,16), SXF_NOCHECKPOSITION|SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION)//|SXF_ABSOLUTEANGLE)
	  BLOD A 0 A_Jump(8, "Death")
	  BLOD A 0 //A_PlaySound("flesh/gush")
	  BLOD A 0 //A_SetAngle(random(-180,180))
      BL0D FFFF 1 Bright A_SpawnItemEx("BludEffect2", 0,0,0, 0,0,0, random(-24,24), SXF_NOCHECKPOSITION|SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION|SXF_ABSOLUTEANGLE)//A_CustomMissile("xBludEffect2",0,0,random(-180,180),2,random(-90,75))
      Stop
  Death:
    TNT1 AA 0
	Stop
  }
}

