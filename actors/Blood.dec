ACTOR BludEffect1
{
	+NOTELEPORT
  Radius 1
  Height 1
  Speed 0//1
  Damage 0
  Projectile
  -RANDOMIZE
  -NOCLIP
  +THRUACTORS
  +DONTSPLASH
  +NOPAIN
  +PAINLESS
  RenderStyle Translucent
  Alpha 0.51
  scale 0.75
  States
  {
  Spawn:
    BLOD A 0
    BLOD A 0 A_Jump(256, 2)//127
	BLOD A 0
	Stop
	
  Prefade:
    BLOD A 0 A_Jump(64, "WhichSpawn")
    BLOD A 0 A_FadeOut(0.01)
    Loop
	
  WhichSpawn:
    BLOD A 0 //A_SetPitch(random(-90,60))
    BLOD A 0 A_Jump(96, "Spawn2", "Spawn3")
	Goto Spawn1
	
  Spawn1:
    BLOD AAAABBBBCCCCCCCC 2 A_FadeOut(0.04)
    stop	
  Spawn2:
    BLOD AAABBBCCCCCCC 2 A_FadeOut(0.05)
    stop	
  Spawn3:
    BLOD AABBCCCCCC 2 A_FadeOut(0.06)
    stop
	
  Death:
  XDeath:
  Crash:
    BLOD C 0 A_Stop
   NULL A 0 A_SpawnItemEx ("dripsound", 0, 0, 0, 0, 0, 0, 0, 128, 0)
  TNT1 A 0 A_CheckFloor("Death2")
  TNT1 AA 1
  Stop
 Death2:
  TNT1 A 1 A_SpawnItemEX("TinyBloodSpot", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION|SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION)
  TNT1 A 0
  Stop
	Stop
  }
}

ACTOR BludEffect1c : BludEffect1
{
  -NOGRAVITY
  Gravity 0.75
  //Alpha 0.85
  States
  {
    WhichSpawn:
    BLOD A 0 //A_SetPitch(random(-90,60))
	BLUD C 0 ThrustThingZ(0, random(0,48), 0, 0)
	BLOD A 0 A_Recoil (random(-10,-4))
    BLOD A 0 A_Jump(96, "Spawn2", "Spawn3")
	Goto Spawn1
  }
}

ACTOR BludEffect1b : BludEffect1
{
  Alpha 0.51
  scale 1.25
  States
  {
  Spawn:
    BLOD A 0
    BLOD A 0 A_Jump(64, "Death")
	
  Prefade:
    BLOD A 0 A_Jump(16, "WhichSpawn")
    BLOD A 0 A_FadeOut(0.01)
    Loop	


  WhichSpawn:
    BLOD ABC 1 A_FadeOut(0.07)
    BLOD A 0 A_Jump(128, "Spawn2", "Spawn3")
	
  Spawn1:
    BLOD C 3 A_FadeOut(0.01)
    BLOD C 0 A_Jump(30, "Death")
	Loop
  Spawn2:
    BLOD C 2 A_FadeOut(0.01)
    BLOD C 0 A_Jump(20, "Death")
	Loop
  Spawn3:
    BLOD C 1 A_FadeOut(0.01)
    BLOD C 0 A_Jump(10, "Death")
	Loop
	
  Death:
  XDeath:
  Crash:
    BLOD C 0 A_Stop
    BLOD C 1 A_FadeOut(0.2)
	Loop
  }
}

ACTOR BludEffect2 : BludEffect1c
{
  Speed 8
  scale 0.1
}

ACTOR BludEffect3 : BludEffect1c
{
  Speed 10
  scale 0.2
}

ACTOR BludEffect4 : BludEffect1c
{
  Speed 12
  scale 0.3
}






actor TinyBloodSpot : SmallBloodPool
{
 +FLOORHUGGER
 radius 1
 height 1
 gravity 100
 health 5//10
 renderstyle translucent
 alpha 0.6//.77
 scale 0.5
 Seesound "BLD/SPLS"
 +DONTSPLASH
 +ALLOWCLIENTSPAWN
 +CLIENTSIDEONLY
 +NOTELEPORT
  states
  {
  Spawn:
   BLUD C 0
   BLUD C 0 A_SetAngle(random(-180,180))
    	//TNT1 A 0 Thing_ChangeTID (0, 1330) 

    BLOD A 0 A_Jump(127, "SpawnB","SpawnC")
	Goto SpawnA
	
  SpawnA:
    POB2 AAAAAAAAAAAA 10 //15
	Goto Death
  SpawnB:
    POB2 BBBBBBBBBBBB 10 //15
	Goto Death
  SpawnC:
    POB2 CCCCCCCCCCCC 10 //15
	Goto Death
  Death:
    POB2 A 2 A_Fadeout(0.01)
    Loop
  }
}

ACTOR dripsound
{
   Radius 1
   Height 1
   +NOBLOCKMAP
   +NOCLIP
  +DONTSPLASH
   +ALLOWCLIENTSPAWN
   States
   {
   Spawn:
      NULL A 0
      NULL A 1 A_Playsound ("BLD/SPLS")
      Stop
   }
}