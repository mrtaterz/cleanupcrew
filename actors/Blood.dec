
ACTOR AeniBlood : Blood replaces Blood
{
  SpawnID 130
  Mass 5
  +NOBLOCKMAP
  +NOTELEPORT
  -ALLOWPARTICLES
  +NOGRAVITY
  +PUFFGETSOWNER
  Scale 0.25
  Alpha 0.5
  States
  {
  Spawn:
  Spray:
    BLOD C 0
    BLOD C 0 A_FaceTarget
    BLOD C 0 A_SpawnItemEx ("AeniBlood_StupidNetcode", 0,0,0, 0,0,0, 0, SXF_NOCHECKPOSITION|SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION|SXF_TRANSFERPOINTERS, 0)
	Stop
  }
}

ACTOR AeniBlood_StupidNetcode : AeniBlood
{
  States
  {
  Spawn:
  Spray:
    BLOD C 0
    BLOD C 0 A_FaceTarget
    BLOD C 0 A_Jump(176,"Spawn2")
    BLOD C 0 A_Jump(256,"Spawn1")
	//BL0D A 35
	//Stop
	
  Spawn1:
    BLOD C 0 A_SpawnItemEx ("AeniBloodEffectSpawner", 0,0,0, 0,0,0, random(-180,180), SXF_NOCHECKPOSITION|SXF_CLIENTSIDE|SXF_ABSOLUTEANGLE|SXF_TRANSFERTRANSLATION|SXF_TRANSFERPOINTERS, 0)
    BLOD C 0 A_SpawnItemEx ("AeniBlood2", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION|SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION|SXF_TRANSFERPOINTERS, 0)
	Stop
	
  Spawn2:	
    BLOD C 0 A_SpawnItemEx ("AeniSprayEffectSpawner", 0,0,0, 0,0,0, random(-180,180), SXF_NOCHECKPOSITION|SXF_CLIENTSIDE|SXF_ABSOLUTEANGLE|SXF_TRANSFERTRANSLATION|SXF_TRANSFERPOINTERS, 0)
    BLOD C 0 A_SpawnItemEx ("AeniBloodSpray", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION|SXF_CLIENTSIDE|SXF_TRANSFERTRANSLATION|SXF_TRANSFERPOINTERS, 0)
    Stop
  }
}

ACTOR AeniBloodSpray : AeniBlood
{
